{
  "name": "localing-api",
  "version": "1.0.0",
  "description": "Localing API",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test": "mocha --require @babel/register --exit",
    "test:debug": "mocha --require @babel/register --inspect-brk --exit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "aws-sdk": "^2.739.0",
    "axios": "^0.21.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cognito-express": "^2.0.18",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.8.0",
    "pino": "^6.4.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
